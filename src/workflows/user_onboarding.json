{
    "workflow_id": "user_onboarding",
    "nodes": {
      "trigger": {
        "type": "trigger",
        "config": {}
      },
      "fetch_user": {
        "type": "data_fetch",
        "config": {
          "query": "SELECT * FROM users WHERE id = ${input.user_id}"
        }
      },
      "enrich_profile": {
        "type": "api_request",
        "config": {
          "url": "https://api.example.com/enrich",
          "method": "POST",
          "body": {
            "email": "${fetch_user.email}"
          }
        }
      },
      "send_welcome": {
        "type": "send_email",
        "config": {
          "to": "${fetch_user.email}",
          "subject": "Welcome!",
          "template": "welcome_email"
        }
      }
    },
    "edges": [
      { "from": "trigger", "to": "fetch_user" },
      { "from": "fetch_user", "to": "enrich_profile" },
      { "from": "enrich_profile", "to": "send_welcome" }
    ]
  }
  